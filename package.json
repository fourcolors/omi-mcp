	"devDependencies": {
		"@types/node": "^20.12.12",
		"concurrently": "^8.2.2",
		"nodemon": "^3.1.0",
		"typescript": "^5.5.2",
		"vitest": "~3.0.7"
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.1.1",
		"dotenv": "^16.4.5",
		"zod": "^3.22.4"
	}
}
